msgid "frontpage"
msgstr "Frontpage Server Extensions"

msgid "frontpage_help"
msgstr "Frontpage Server Extensions are used so that webs may be published using the Frontpage client application.  Frontpage makes simple graphic design and web forms easy to implement."

msgid "frontpageSettings"
msgstr "Frontpage Server Extensions Settings"

msgid "systemEnabled"
msgstr "Enable the Root Web"

msgid "systemEnabled_help"
msgstr "Check to enable Frontpage publishing for the home group web site."

msgid "passwd"
msgstr "Webmaster password"

msgid "passwd_help"
msgstr "Open this web using the Frontpage user webmaster and this password."

msgid "passwd_empty"
msgstr "Please enter the webmaster password."

msgid "fpx_failed"
msgstr "A FrontPage Root Web could not be enabled"

msgid "fpxEnabled"
msgstr "FrontPage Server Extensions"

msgid "fpxEnabled_help"
msgstr "FrontPage Server Extensions are enabled for this user."

