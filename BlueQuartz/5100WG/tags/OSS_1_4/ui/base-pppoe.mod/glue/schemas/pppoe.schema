
<class name="System" namespace="Pppoe" version="1.0">
	<!-- Basic properties -->
	<property name="ethNumber" type="int" default="1" writeacl="ruleCapable(modifySystemNetwork)"/>
	<property name="connMode"  type="scalar" default="off" writeacl="ruleCapable(modifySystemNetwork)"/>
	<property name="userName"  readacl="ruleSelf,ruleAdmin" type="scalar" writeacl="ruleCapable(modifySystemNetwork)"/>
	<property name="password"  readacl="ruleSelf,ruleAdmin" type="scalar" writeacl="ruleCapable(modifySystemNetwork)"/>
</class>
