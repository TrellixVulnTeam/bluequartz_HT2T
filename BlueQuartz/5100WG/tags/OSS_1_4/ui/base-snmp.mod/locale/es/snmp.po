# --- SNMP page

msgid "enableServer"
msgstr "Habilitar servidor"

msgid "enableServer_help"
msgstr "Activar o desactivar el servidor SNMP (protocolo simple de administración de red), que permite supervisar y controlar los dispositivos de red y administrar las configuraciones, la recopilación de estadísticas, el rendimiento y la seguridad de una red."

msgid "readSnmpCommunityField"
msgstr "Comunidad SNMP de sólo lectura"

msgid "readSnmpCommunityField_rule"
msgstr "La comunidad SNMP de sólo lectura sólo puede contener caracteres alfanuméricos y signos '-' y '_'."

msgid "readSnmpCommunityField_help"
msgstr "Comunidad SNMP de sólo lectura a la que pertenece este servidor. [[base-snmp.readSnmpCommunityField_rule]]"

msgid "readSnmpCommunityField_invalid"
msgstr "[[VAR.invalidValue]] no es un valor válido para [[base-snmp.readSnmpCommunityField]]. [[base-snmp.readSnmpCommunityField_rule]]"

msgid "readWriteSnmpCommunityField"
msgstr "Comunidad SNMP de lectura y escritura"

msgid "readWriteSnmpCommunityField_rule"
msgstr "La comunicad SNMP de lectura y escritura sólo puede contener caracteres alfanuméricos y los signos '-' y '_'."

msgid "readWriteSnmpCommunityField_help"
msgstr "Comunidad SNMP de lectura y escritura a la que pertenece este servidor. [[base-snmp.readWriteSnmpCommunityField_rule]]"

msgid "readWriteSnmpCommunityField_invalid"
msgstr "[[VAR.invalidValue]] no es un valor válido para [[base-snmp.readWriteSnmpCommunityField]]. [[base-snmp.readWriteSnmpCommunityField_rule]]"

