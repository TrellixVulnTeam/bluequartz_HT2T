msgid "zone_format_16_help"
msgstr "Definieren Sie das für ein Oktett-Grenzen-unabhängiges Netzwerk mit mehr als 16 Bit zu erstellende [[base-dns.zone_format]].  [[base-dns.zone_format_required_warning]]"

msgid "zone_format_8"
msgstr "[[base-dns.zone_format]] für Netzwerke > 8 Bit."

msgid "zone_format_8_help"
msgstr "Definieren Sie das für ein Oktett-Grenzen-unabhängiges Netzwerk mit mehr als 8 Bit zu erstellende [[base-dns.zone_format]].  [[base-dns.zone_format_required_warning]]"

msgid "zone_format_0"
msgstr "[[base-dns.zone_format]] für Netzwerke > 0 Bit."

msgid "zone_format_0_help"
msgstr "Definieren Sie das für ein Oktett-Grenzen-unabhängiges Netzwerk mit mehr als 0 Bit zu erstellende [[base-dns.zone_format]].  [[base-dns.zone_format_required_warning]]"

