    modifyTelnet capability used to limit access to to properties for
    the Vsite(Services).Telnet namespace and system wide telnet server
    control
-->

<class
    name="Capabilities" namespace="modifyTelnet" version="1.0"
>
    <property
        name="capable" type="boolean" default="1"
        readacl="ruleCapable(modifyTelnet)"
    />
    <property
        name="nameTag" type="scalar" default=""
    />
</class>
