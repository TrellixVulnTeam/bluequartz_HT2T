# Summary

msgid "genDate"
msgstr "Usage Last Checked"

msgid "genDate_help"
msgstr "This is the last time web usage statistics were generated."

msgid "periodStart"
msgstr "Report Coverage Period (From)"

msgid "periodStart_help"
msgstr "This is the start of the period covered by these web usage statistics."

msgid "periodEnd"
msgstr "Report Coverage Period (To)"

msgid "periodEnd_help"
msgstr "This is the end of the period covered by these web usage statistics."

msgid "totalHits"
msgstr "Total Number of Requests"

msgid "totalHits_help"
msgstr "This is the total number of web requests or hits generated during the report period."

msgid "badHits"
msgstr "Number of Bad Requests"

msgid "badHits_help"
msgstr "This is the number of bad web requests or hits generated during the report period.  These are requests for web pages that cannot be fulfilled for various reasons.  For example, these requests may have been made for web pages which do not exist on this web server."

msgid "numIPs"
msgstr "Number of Unique Computers Making Requests"

msgid "numIPs_help"
msgstr "This is the number of unique computers requesting web pages during the report period."

msgid "numFiles"
msgstr "Number of Unique Pages Requested"

msgid "numFiles_help"
msgstr "This is the number of unique web pages requested during the report period."

msgid "numRefs"
msgstr "Number of Unique Referring Pages"

msgid "numRefs_help"
msgstr "This is the number of unique referring pages during the report period.  Referring pages are web pages that contain hyperlinks to the web pages on this web server."

msgid "numBytes"
msgstr "Number of Bytes Downloaded"

msgid "numBytes_help"
msgstr "This is the number of bytes downloaded during the report period."

# Most Active Requesters

msgid "hostname"
msgstr "Requester Name"

msgid "hostname_help"
msgstr "This is the host name of the computer that requested web pages from this web server."

msgid "hits"
msgstr "Requests"

msgid "hits_help"
msgstr "This is the number of web requests or hits generated by the Requester."

msgid "referrer"
msgstr "Referrer"

msgid "referrer_help"
msgstr "This is the web page that referred the Requester to the web pages on this web server."

# Most Requested Pages

msgid "file"
msgstr "Page"

msgid "file_help"
msgstr "This is the web page being requested by the Requester."

msgid "pagehits"
msgstr "Requests"

msgid "pagehits_help"
msgstr "This is the number of web requests or hits for this web page."

