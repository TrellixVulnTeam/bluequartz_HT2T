# ---- LDAP directory/export page

msgid "ldapExport"
msgstr "Directorio [[base-ldap.ldapName]]"

msgid "ldapExportDescription"
msgstr "Configurar el directorio de usuarios con el servicio [[base-ldap.ldapName]]."

msgid "exportEnabled"
msgstr "Habilitar"

msgid "exportEnabled_help"
msgstr "Activar o desactivar el servicio [[base-ldap.ldapName]], con el que los usuarios podrán usar clientes LDAP estándar para obtener direcciones de correo electrónico y otros datos sobre los usuarios de este servidor."

msgid "baseDn"
msgstr "Nombre distintivo (Dn) básico"

msgid "baseDn_rule"
msgstr "Por ejemplo, o=Mi empresa, c=España."

msgid "baseDn_help"
msgstr "Nombre distintivo (Dn) básico de su directorio de usuarios. [[base-ldap.baseDn_rule]]"

msgid "baseDn_empty"
msgstr "No se ha especificado un valor para [[base-ldap.baseDn]]. [[base-ldap.baseDn_rule]]"

msgid "emailBase"
msgstr "Nombre de dominio de correo electrónico"

msgid "emailBase_help"
msgstr "Nombre de dominio alternativo para las direcciones de correo electrónico exportadas a su directorio. Si el nombre de este servidor es correo.empresa.com, pero recibe correo de @empresa.com, tal vez sea deseable definir el nombre de dominio de correo empresa.com. Así todas las direcciones de correo del directorio tendrán el formato usuario@empresa.com y no usuario@correo.empresa.com."

msgid "emailBase_invalid"
msgstr "[[VAR.invalidValue]] no es un valor válido para [[base-ldap.emailBase]]. Escribir un nombre de ordenador o de dominio con el formato apropiado."


# ------ LDAP Import section 

msgid "ldapImport"
msgstr "Importación de [[base-ldap.ldapName]]"

msgid "import"
msgstr "Importar ahora"

msgid "import_help"
msgstr "Pulse aquí para importar usuarios desde el servidor de [[base-ldap.ldapName]]."

msgid "server"
msgstr "Dirección de red del servidor"

msgid "server_help"
msgstr "Dirección IP o nombre del servidor desde el que desea importar usuarios. [[base-ldap.server_rule]]"

msgid "server_rule"
msgstr "Por ejemplo, cobalt.com o 192.168.100.23." 

msgid "server_empty"
msgstr "No se ha especificado un valor para [[base-ldap.server]]."

msgid "server_invalid"
msgstr "[[VAR.invalidValue]] no es un valor válido para [[base-ldap.server]]. Escribir un nombre de ordenador o una dirección IP válido. [[base-ldap.server_rule]]"

msgid "base"
msgstr "Nombre distintivo (Dn) básico"

msgid "base_help"
msgstr "Base de las entradas LDAP para importar."

msgid "base_empty"
msgstr "No se ha especificado un valor para [[base-ldap.base]]."

msgid "bindDn"
msgstr "Nombre distintivo (Dn) de enlace"

msgid "bindDn_help"
msgstr "Nombre distintivo (Dn) de enlace del usuario administrador del servidor LDAP. Este nombre y su contraseña son necesarios para autenticar y adquirir información para la importación de usuarios. Por ejemplo, cn=minombre, C=España."

msgid "bindDn_rule"
msgstr "Nombre distintivo (Dn) de enlace con el formato apropiado. Por ejemplo, cn=minombre, C=España."

msgid "bindDn_empty"
msgstr "No se ha especificado un valor para [[base-ldap.bindDn]]. [[base-ldap.bindDn_rule]]"

msgid "passwordAuth"
msgstr "Contraseña"

msgid "passwordAuth_help"
msgstr "Contraseña para el nombre distintivo (Dn) de enlace"

msgid "passwordAuth_empty"
msgstr "No se ha especificado un valor para [[base-ldap.passwordAuth]]."

msgid "passwordAuth_invalid"
msgstr "No se ha escrito la misma contraseña en ambos campos. Inténtelo de nuevo." 

msgid "userFilter"
msgstr "Filtro de usuarios"

msgid "userFilter_help"
msgstr "Filtro utilizado para determinar qué clases de objetos importar como usuarios. El valor en blanco es (|(objectClass=cobaltAccount)(objectClass=posixAccount))"

msgid "groupFilter"
msgstr "Filtro de grupos"

msgid "groupFilter_help"
msgstr "Filtro utilizado para determinar qué clases de objetos importar como grupos. El valor en blanco es (|(objectClass=cobaltGroup)(objectClass=posixGroup))"



