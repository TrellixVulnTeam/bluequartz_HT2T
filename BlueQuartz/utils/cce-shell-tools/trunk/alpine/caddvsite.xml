<?xml version = "1.0" encoding = "iso-8859-1"?>
	<caddvsite>
		<mailAliases>
			<opts value = "mailAliases=s mail>mailAliases e>mailAliases"/>
			<label value = "mailAliases"/>
			<type value = "array"/>
			<help value = "accept email for domains, comma or space delmited list"/>
		</mailAliases>
		<webAliases>
			<opts value = "webAliases=s web>webAliases w>webAliases"/>
			<label value = "webAliases"/>
			<type value = "array"/>
			<help value = "web access for domains, comma or space delmited list"/>
		</webAliases>
		<volume>
			<opts value = "volume=s v>volume"/>
			<label value = "volume"/>
			<help value = "volume for the site"/>
		</volume>
		<fpx>
			<opts value = "fpx=s frontpage>fpx x>fpx enable-fpx>fpx"/>
			<label value = "passwordWebmaster"/>
			<nameSpace value = "Frontpage"/>
			<help value = "enable fronpage with supplied password"/>
			<ref>
				<label value = "enabled"/>
				<labelValue value = "1"/>	
			</ref>
		</fpx>
		<apop>
			<opts value = "apop o>apop enable-apop>apop"/>
			<label value = "enabled"/>
			<nameSpace value = "APOP"/>
			<help value = "apop enabled"/>
		</apop>
 		<help>
			<opts value = "help h>help ?>help"/>
			<label value = "help"/>
			<help value = "print help text"/>
		</help>
		<quota>
			<opts value = "quota=i q>quota"/> 
			<label value = "quota"/>
			<nameSpace value = "Disk"/>
			<help value = "site quota in MB"/>
		</quota>
		<fqdn>
			<opts value = "fqdn=s domain>fqdn d>fqdn"/>
			<label value = "fqdn"/>
			<help value = "fully quilified domain name ie. www.domain.com"/>
		</fqdn>
		<dnsauto>
			<opts value = "dnsauto dns>dnsauto d>dnsauto auto-dns>dnsauto"/>
			<label value = "auto_dns"/>
			<help value = "Automatic DNS configuration"/>
		</dnsauto>
		<maxusers>
			<opts value = "maxusers=i u>maxusers max>maxusers"/>
			<label value = "maxusers"/>
			<help value = "maximun number of users in virtual site"/>
		</maxusers>
		<shell>
			<opts value = "shell t>shell telnet>shell enable-telnet>shell enable-shell>shell"/> 
			<label value = "enabled"/>
			<nameSpace value = "Telnet"/>
			<help value = "shell (telnet) enabled"/>
		</shell>
		<java>
			<opts value = "java j>java enable-java>java"/> 
			<label value = "enabled"/>
			<nameSpace value = "Java"/>
			<help value = "Java enabled"/>
		</java>
		<ftp>
			<opts value = "ftp f>ftp enable-ftp>ftp"/> 
			<label value = "enabled"/>
			<nameSpace value = "AnonFtp"/>
			<help value = "anonymous ftp enabled"/>
		</ftp>
		<ftpQuota>
			<opts value = "ftpQuota=i m>ftpQuota ftp-quota>ftpQuota"/> 
			<label value = "quota"/>
			<nameSpace value = "AnonFtp"/>
			<help value = "anonymous ftp quota"/>
		</ftpQuota>
		<ftpUsers>
			<opts value = "ftpUsers=i z>ftpUsers ftp-users>ftpUsers"/> 
			<label value = "maxConnections"/>
			<nameSpace value = "AnonFtp"/>
			<help value = "anonymous ftp number of simultaneous users"/>
		</ftpUsers>
		<cgi>
			<opts value = "cgi c>cgi enable-cgi>cgi"/> 
			<label value = "enabled"/>
			<nameSpace value = "CGI"/>
			<help value = "cgi scripts enabled"/>
		</cgi>
		<ssi>
			<opts value = "ssi s>ssi enabl-ssi>ssi"/> 
			<label value = "enabled"/>
			<nameSpace value = "SSI"/>
			<help value = "Server side includes enabled"/>
		</ssi>
		<php>
			<opts value = "php p>php enable-php>php"/> 
			<label value = "enabled"/>
			<nameSpace value = "PHP"/>
			<help value = "php enabled"/>
		</php>
		<asp>
			<opts value = "asp a>asp enable-asp>asp"/> 
			<label value = "enabled"/>
			<nameSpace value = "ASP"/>
			<help value = "Active Server Pages (ASP) enabled"/>
		</asp>
		<ssl>
			<opts value = "ssl l>ssl enable-ssl>ssl"/> 
			<label value = "enabled"/>
			<nameSpace value = "Ssl"/>
			<help value = "Secure Socket Layer (SSL) enabled"/>
		</ssl>
		<ipaddr>
			<opts value = "ipaddr=s i>ipaddr ipaddress>ipaddr"/> 
			<label value = "ipaddr"/>
			<help value = "IP address of the virtual site"/>
		</ipaddr>
	</caddvsite>
