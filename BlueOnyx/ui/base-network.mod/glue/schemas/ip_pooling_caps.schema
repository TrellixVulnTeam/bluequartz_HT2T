<class
    name="Capabilities" namespace="ipPooling" version="0.1"
>
    <property
        name="capable" type="boolean" default="1"
        readacl="ruleCapable(ipPooling)"
    />
    <property
        name="nameTag" type="scalar" default="[[base-network.pooling_block]]"
    />
</class>
