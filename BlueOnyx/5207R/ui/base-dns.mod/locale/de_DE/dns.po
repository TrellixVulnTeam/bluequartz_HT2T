msgid "a_domain_name_help"
msgstr "Geben Sie den Domänennamen für diesen Datensatz ein. [[base-dns.a_record_explain]]"

msgid "a_domain_name_invalid"
msgstr "Der angegebene Domänenname enthält ungültige Zeichen."

msgid "a_domain_name_empty"
msgstr "Geben Sie die Domäne für diesen Datensatz ein."

msgid "a_ip_address"
msgstr "IP-Addresse"

msgid "a_ip_address_help"
msgstr "Geben Sie die IP-Adresse für diesen Datensatz ein. [[base-dns.a_record_explain]]"

msgid "a_ip_address_invalid"
