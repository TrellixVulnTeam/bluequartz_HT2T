msgid "zone_format_8_help"
msgstr "Defina el [[base-dns.zone_format]] para crear una red de límite no octeto con más de 8 bits aquí. [[base-dns.zone_format_required_warning]]"

msgid "zone_format_0"
msgstr "[[base-dns.zone_format]]&gt; Redes de 0 bits."

msgid "zone_format_0_help"
msgstr "Defina el [[base-dns.zone_format]] para crear una red de límite no octeto con más de 0 bits de aquí. [[base-dns.zone_format_required_warning]]"

