msgid "create_soa"
msgstr "Ajouter enregistrement SOA (Start of Authority)"

msgid "modify_soa"
msgstr "[[base-dns.title]] Modifier enregistrement SOA (Start of Authority)"

msgid "domain_soa"
msgstr "Domaine concerné"

msgid "domain_soa_help"
msgstr "Domaine auquel cet enregistrement SOA s'applique."

msgid "network_soa"
msgstr "Autorité réseau"

msgid "network_soa_help"
msgstr "Réseau auquel cet enregistrement SOA s'applique."

msgid "primary_dns"
msgstr "Serveur de noms (NS) maître ou primaire"

msgid "primary_dns_help"
msgstr "Spécifier le nom de domaine entièrement qualifié FQDN du serveur de noms primaire pour le domaine ou l'autorité de réseau concerné."

msgid "secondary_dns"
msgstr "Serveurs de noms (NS) secondaires"

msgid "secondary_dns_help"
msgstr "Saisissez un ou plusieurs noms de domaine entièrement qualifié FQDNs des serveurs de noms secondaires pour le domaine ou l'autorité de réseau concerné. Séparez les noms de domaine par des espaces."

msgid "domain_admin"
