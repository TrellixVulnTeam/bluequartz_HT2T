# ----- Menu
msgid "multidrop"
msgstr "远程检索"

msgid "multidropHelp"
msgstr "远程电子邮件检索设置可在此处配置。"

# ----- Remote Retrieval (aka multidrop)page

msgid "modifyMultidrop"
msgstr "远程检索设置"

msgid "enableField"
msgstr "启用远程检索"

msgid "enableField_help"
msgstr "打开或关闭远程检索。远程检索（或多点）可用于定期从远程服务器检索电子邮件。请参考您的服务器设备手册，了解详细信息。"

msgid "serverField"
msgstr "远程电子邮件服务器"

msgid "serverField_eg"
msgstr "例如，192.168.1.100 和 mail.cobalt.com 就是有效的输入。"

msgid "serverField_rule"
msgstr "请为[[base-multidrop.serverField]]输入格式正确的网络地址或完全限定的域名。[[base-multidrop.serverField_eg]]"

msgid "serverField_help"
msgstr "请输入检索整个域电子邮件的远程电子邮件服务器网络地址或完全限定的域名。 [[base-multidrop.serverField_eg]]"

msgid "userDomainField"
msgstr "电子邮件域名"

msgid "userDomainField_rule"
msgstr "请输入[[base-multidrop.userDomainField]]，表示格式正确的网络地址或完全限定的域名。例如，192.168.1.100 和 mail.cobalt.com 就是有效的输入"

msgid "userDomainField_help"
msgstr "输入您的电子邮件要发送的域名。例如，如果电子邮件存储到多点邮箱的一个地址是 joe@example.com，域名将是 example.com"

msgid "userNameField"
msgstr "用户名"

msgid "userNameField_help"
msgstr "请输入检索您的整个域电子邮件的远程电子邮件服务器帐户的用户名。"

msgid "userNameField_rule"
msgstr "请输入一个不多于 12 个字符的用户名，其中只包含小写字母数字字符、句点、连字符和下划线。"

msgid "passwordField"
msgstr "口令"

msgid "passwordField_help"
msgstr "请输入检索您的整个域电子邮件的远程电子邮件服务器帐户的口令。"

msgid "passwordField_invalid"
msgstr "对不起，您没有在两个字段中输入相同的口令。两个字段中输入相同口令是为了防止键入错误。请重试。"

msgid "intervalField"
msgstr "检索频率"

msgid "intervalField_rule"
msgstr "请输入一个大于 0 的数字表示时间间隔"

msgid "intervalField_help"
msgstr "输入从远程电子邮件服务器检索进来的电子邮件的频率（分钟）。"

msgid "protoField"
msgstr "消息检索协议"

msgid "protoField_help"
msgstr "选择从远程电子邮件服务器检索电子邮件的方法。ETRN 方法只用于您的远程电子邮件服务器是 ESMTP 兼容型时使用。缺省设置 POP3 应适用于大多数用户。"

msgid "badInterval"
msgstr "[[base-multidrop.intervalField]]必须大于 0。"

