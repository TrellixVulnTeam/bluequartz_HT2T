<cobaltBase>
  <name value = "Cobalt base configuration"/>
  <product value =  "Qube3"/>
  <version value = "[VERSION]"/>
  <versionRequired value = "2.29"/>
  <libs>
    <scanout value = "/usr/cmu/scripts/Qube3scanout.pl"/>
	<adjust value = "/usr/cmu/scripts/Qube3adjust.pl"/>
	<conflict value = "/usr/cmu/scripts/Qube3conflict.pl"/>
	<scanin value = "/usr/cmu/scripts/Qube3scanin.pl"/>
  </libs>
  <cmuXml value = "cmu.xml"/>
  <tmpDir value = "/home/tmp"/>
  <cmuDir value = "/home/cmu"/>
  <cmuUser value = "root"/>
  <cmuGroup value = "admin"/>
  <libDir value = "/usr/cmu/perl"/>
  <logFile value = "/home/cmu/cmu.log"/>
  <lockFile value = "/home/cmu/cmu.lock"/>
  <skipAdjust value = "f"/>
  <skipConflict value = "f"/>
  <confOnly	value = "f"/>
  <noPasswd value = "f"/>
  <adminFiles value = "f"/>
  <webEnabled value = "f"/>
  <userPasswd value = "123456"/>
  <debug value = "f"/>
  <conflict>
	<userName>
		<class value = "user"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "checkAll"/>
		<validate value = "isUserQube"/>
	</userName>
	<userNameNumber>
		<class value = "user"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "checkAll"/>
		<validate value = "isUserQube"/>
	</userNameNumber>
	<userNameLength>
		<class value = "user"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "checkAll"/>
		<validate value = "isUserQube"/>
	</userNameLength>
	<userAlias>
		<class value = "alias"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
		</actions>
	</userAlias>
	<groupName>
		<class value = "group"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "checkAll"/>
		<validate value = "isGroupQube"/>
	</groupName>
	<groupNameNumber>
		<class value = "group"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "checkAll"/>
		<validate value = "isGroupQube"/>
	</groupNameNumber>
	<listName>
		<class value = "list"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "detectMailList"/>
		<validate value = "isGroupQube"/>
	</listName>
	<listNameNumber>
		<class value = "list"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "checkAll"/>
		<validate value = "isGroupQube"/>
	</listNameNumber>
  </conflict>
</cobaltBase>
