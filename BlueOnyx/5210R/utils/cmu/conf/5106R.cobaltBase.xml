<cobaltBase>
  <name value = "Cobalt base configuration"/>
  <product value =  "5106R"/>
  <version value = "[VERSION]"/>
  <versionRequired value = "2.29"/>
  <libs>
    <scanout value = "/usr/cmu/scripts/5106Rscanout.pl"/>
    <adjust value = "/usr/cmu/scripts/5106Radjust.pl"/>
    <conflict value = "/usr/cmu/scripts/5106Rconflict.pl"/>
	<scanin value = "/usr/cmu/scripts/5106Rscanin.pl"/>
  </libs>
  <cmuXml value = "cmu.xml"/>
  <tmpDir value = "/home/tmp"/>
  <cmuDir value = "/home/cmu"/>
  <cmuUser value = "root"/>
  <cmuGroup value = "root"/>
  <libDir value = "/usr/cmu/perl"/>
  <logFile value = "/home/cmu/cmu.log"/>
  <lockFile value = "/home/cmu/cmu.lock"/>
  <skipAdjust value = "f"/>
  <skipConflict value = "f"/>
  <conflictOnly	value = "f"/>
  <noPasswd value = "f"/>
  <confOnly value = "f"/>
  <superUsers value = "f"/>
  <adminFiles value = "f"/>
  <userPasswd value = "123456"/>
  <dns value = "f"/>
  <webEnabled value = "f"/>
  <debug value = "f"/>
  <conflict>
	<vsiteFqdn>
		<class value = "vsite"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
			<Merge value = "mergeClass"/>
		</actions>
		<detector value = "detectVsiteFqdn"/>
		<validate value = "isFqdn"/>
	</vsiteFqdn>
	<ipService>
		<class value = "vsite"/> 
		<defaultAction value = "Deactivate"/>
		<actions>
			<Quit value = "quit"/>
			<Deactivate value = "deactVal"/>
		</actions>
	</ipService>
	<domainAlias>
		<class value = "vsite"/> 
		<defaultAction value = "Discard"/>
		<actions>
			<Quit value = "quit"/>
			<Discard value = "discardVal"/>
		</actions>
	</domainAlias>
	<userName>
		<class value = "user"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<validate value = "isUserQube"/>
		<detector value = "detectUserName"/>
	</userName>
	<userNameNumber>
		<class value = "user"/> 
		<defaultAction value = "Drop"/>
		<actions>
			<Quit value = "quit"/>
			<Drop value = "dropClass"/>
			<Change value = "changeClass"/>
		</actions>
		<detector value = "detectUserName"/>
		<validate value = "isUserQube"/>
	</userNameNumber>
	<userQuota>
		<class value = "user"/> 
		<defaultAction value = "changeVal"/>
		<actions>
			<Quit value = "quit"/>
		<!--<Change value = "changeVal"/> -->
		</actions>
		<validate value = "isInt"/>
	</userQuota>
  </conflict>
</cobaltBase>
