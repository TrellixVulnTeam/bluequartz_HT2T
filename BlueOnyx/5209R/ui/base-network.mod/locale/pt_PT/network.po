msgid "dhcpClientIdField"
msgstr "Identificador cliente"

msgid "dhcpClientIdField_help"
msgstr "Digite a seqüência de identificação que identifica este servidor como um cliente Dynamic Host Configuration Protocol (DHCP)."

msgid "pppoe"
msgstr "Obter automaticamente usando o protocolo Point-to-Point Over Ethernet (PPPoE)"

msgid "pppUserNameField"
msgstr "Nome de usuário"

msgid "pppUserNameField_help"
msgstr "Digite o nome de usuário utilizado para acessar seu provedor de serviços Internet."

msgid "pppUserNameField_empty"
